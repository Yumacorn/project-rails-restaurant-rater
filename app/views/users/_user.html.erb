<%= render partial: "layouts/errors", locals: {object: @user} %>


<%= form_for(@user) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :email %>
    <%= f.email_field :email %><br>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <%= f.label :password %>
    <%= f.text_field :password %><br>
    <%= f.label :age %>
    <%= f.text_field :age %><br>
    <%= f.label :city %>
    <%= f.text_field :city %><br>
    <%= f.label :state %>
    <%= f.text_field :state %><br>
    <%= f.submit %>
<% end %>
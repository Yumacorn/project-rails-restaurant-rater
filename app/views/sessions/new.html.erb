<h1>Login Authorization Page</h1>
<h3>Log in using external account</h3>
<%= link_to "Log In with Google", '/auth/google_oauth2' %><br>
<%= link_to "Log In with Github", '/auth/github' %><br>
<%#= link_to "Log In with Developer", '/auth/developer' %><br>
<%#= link_to "Log in with Facebook!", '/auth/facebook' %><br>


<h3>Log in using Restaurant Rater</h3>
<%= form_tag(sessions_path) do %>
    <%= label_tag :username %>
    <%= text_field_tag :username, '', placeholder: "Enter your username" %><br>
    <%= label_tag :password %>
    <%= text_field_tag :password, '', placeholder: "Enter your password" %>
    <%= submit_tag "Login" %>
<% end %>


<p>uuu<%= params.inspect %></p>
